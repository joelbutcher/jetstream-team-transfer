{
    "name": "joelbutcher/jetstream-team-transfer",
    "description": "A team transfer form for Laravel Jetstream",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/laravel/jetstream/issues",
        "source": "https://github.com/laravel/jetstream"
    },
    "authors": [
        {
            "name": "Joel Butcher",
            "email": "joel@joelbutcher.co.uk"
        }
    ],
    "require": {
        "illuminate/support": "^8.0",
        "laravel/jetstream": "^1.6|^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "livewire/livewire": "^2.3.5",
        "phpunit/phpunit": "^9.3",
        "inertiajs/inertia-laravel": "^0.3"
    },
    "autoload": {
        "psr-4": {
            "JoelButcher\\JetstreamTeamTransfer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "stubs/app/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "JoelButcher\\JetstreamTeamTransfer\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
